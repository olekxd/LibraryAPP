<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<HBox alignment="CENTER_LEFT" BorderPane.alignment="CENTER" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.bolszowski.mp3player.controller.ControlPaneController">
			<children>
				<Button id="prev" fx:id="prevButton" mnemonicParsing="false" prefHeight="30.0" prefWidth="30.0" HBox.hgrow="NEVER" />
				<ToggleButton id="play" fx:id="playButton" mnemonicParsing="false" prefHeight="30.0" prefWidth="30.0" HBox.hgrow="NEVER" />
				<Button id="next" fx:id="nextButton" mnemonicParsing="false" prefHeight="30.0" prefWidth="30.0" HBox.hgrow="NEVER" />
				<Slider fx:id="volumeSlider" HBox.hgrow="NEVER">
         <padding>
            <Insets left="10.0" right="10.0" />
         </padding></Slider>
				<Slider fx:id="songSlider" HBox.hgrow="ALWAYS">
         <padding>
            <Insets left="10.0" right="10.0" />
         </padding></Slider>
			</children>
	</HBox>
